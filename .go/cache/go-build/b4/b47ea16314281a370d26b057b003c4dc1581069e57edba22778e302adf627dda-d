	rv = func() _Ctype_int{ _cgo0 := /*line :1412:30*/db; var _cgo1 _Ctype_int = _Ctype_int(busyTimeout); _cgoCheckPointer(_cgo0, nil); return _Cfunc_sqlite3_busy_timeout(_cgo0, _cgo1); }()
