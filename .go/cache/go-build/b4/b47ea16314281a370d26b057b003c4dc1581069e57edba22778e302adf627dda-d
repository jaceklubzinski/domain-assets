		rv := func() _Ctype_int{ _cgo0 := /*line :1420:24*/db; var _cgo1 *_Ctype_char = /*line :1420:28*/cs; var _cgo2 *[0]byte = /*line :1420:32*/nil; var _cgo3 _cgo_unsafe.Pointer = /*line :1420:37*/nil; var _cgo4 **_Ctype_char = /*line :1420:42*/nil; _cgoCheckPointer(_cgo0, nil); return _Cfunc_sqlite3_exec(_cgo0, _cgo1, _cgo2, _cgo3, _cgo4); }()
