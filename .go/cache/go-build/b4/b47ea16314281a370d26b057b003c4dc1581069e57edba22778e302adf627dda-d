	}

	// Query Only
	if queryOnly > -1 {
		if err := exec(fmt.Sprintf("PRAGMA query_only = %d;", queryOnly)); err != nil {
			func() _Ctype_int{ _cgo0 := /*line :1650:23*/db; _cgoCheckPointer(_cgo0, nil); return _Cfunc_sqlite3_close_v2(_cgo0); }()
