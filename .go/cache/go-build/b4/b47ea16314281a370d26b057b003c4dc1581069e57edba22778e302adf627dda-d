// Close the rows.
func (rc *SQLiteRows) Close() error {
	rc.s.mu.Lock()
	if rc.s.closed || rc.closed {
		rc.s.mu.Unlock()
		return nil
	}
	rc.closed = true
	if rc.cls {
		rc.s.mu.Unlock()
		return rc.s.Close()
	}
	rv := func() _Ctype_int{ _cgo0 := /*line :2029:24*/rc.s.s; _cgoCheckPointer(_cgo0, nil); return _Cfunc_sqlite3_reset(_cgo0); }()
	if rv != ( /*line :2030:11*/_Ciconst_SQLITE_OK /*line :2030:21*/) {
		rc.s.mu.Unlock()
		return rc.s.c.lastError()
	}
	rc.s.mu.Unlock()
	return nil
}

